{
  "buildCommand": "cd gerador && npm install && npm run build",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/gerador-de-proposta/(.*)",
      "dest": "/gerador/dist/$1"
    },
    {
      "src": "/gerador-de-proposta",
      "dest": "/gerador/dist/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
